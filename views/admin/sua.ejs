<%include ../header%>
<link rel="stylesheet" href="/stylesheets/top.css">
</head>
<body>
	<div class="main">
		<%include ../top%>
		<form method="post" accept-charset="utf-8" enctype="application/x-www-form-urlencoded" class="form-horizontal">
                <input type="hidden" name="idquantridang" value="<%=usr.user.id%>">
                <div class="form-group">
                    <label class="control-label col-sm-2">Tên đầy đủ:</label>
                    <div class="col-sm-10">
                    	<input type="text" class="form-control" name="tendaydu" placeholder="Nhập tên..." value="<%=admin.tendaydu%>">	
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-2">Tên đăng nhập :</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="tendangnhap" placeholder="Nhập tên đăng nhập..." value="<%=admin.tendangnhap%>">   
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-sm-2">Mật khẩu :</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="matkhau" placeholder="Nhập mật khẩu..." value="<%=admin.matkhau%>">   
                    </div>
                </div>
                <div class="form-group">
                	<div class="col-sm-offset-2 col-sm-10">
                		<button type="submit" class="btn btn-success" id="btnSubmit">Sửa</button>	
                	</div>
                </div>
		</form>
		<%include ../footer%>
		<!-- <script src="/javascripts/admin/them.js" type="text/javascript" charset="utf-8"></script> -->
	</div>	
</body>
