<%include ../header%>
</head>
<div class="container table-responsive" style="width: 100%">
    <h1><%=ten%></h1>
    <a href="/<%=ten%>/them" title="thêm <%=ten%>"><button class="btn btn-primary" style="margin: 10px" >Thêm</button></a>
    <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example">
        <thead>
            <tr>
                <th>Id</th>
                <th>Tên</th>
                <th>Quyết định số</th>
                <th>Ký hiệu số</th>
                <th>Ngày ban hành</th>
                <th>Lĩnh vực</th>
                <th>Đường dẫn</th>
                <th>Ghi chú</th>
                <th>Lượt xem</th>
                <th>Id bộ ngành</th>
                <th>Id quản trị đăng</th>
                <th>Id cấp trên</th>
                <th>Thao tác</th>
            </tr>
        </thead>
        <tbody id="admindata">
        	<% for (var i = 0; i < data.length; i++) { -%>
        		<tr>
        			<td><%=data[i].id%></td>
        			<td><%=data[i].ten%></td>
        			<td><%=data[i].quyetdinhso%></td>
        			<td><%=data[i].kyhieuso%></td>
                    <td><%=data[i].ngaybanhanh%></td>
                    <td><%=data[i].linhvuc%></td>
                    <td><%=data[i].duongdan%></td>
                    <td><%=data[i].ghichu%></td>
                    <td><%=data[i].luotxem%></td>
                    <td><%=data[i].idbonganh%></td>
                    <td><%=data[i].idquantridang%></td>
                    <td><%=data[i].idcaptren%></td>
                    <td>
                        <ul>
                            <li>Sửa</li>
                            <li>Xóa</li>
                        </ul>
                    </td>
        		</tr>
        	<% } -%>
        </tbody>
    </table>
    <script src="https://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="/javascripts/admin/jquery.dataTables.min.js"></script>
    <script src="/javascripts/admin/dataTables.bootstrap.js"></script>
    <script src="/javascripts/admin/tables.js"></script>
</div>