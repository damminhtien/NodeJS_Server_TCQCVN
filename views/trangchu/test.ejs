                            <option value=http://www.un.org/en/index.html>Liên Hợp Quốc</option>
                            <option value=http://asean.mofa.gov.vn/>Asean</option>
                            <option value=https://europa.eu/european-union/index_en>Liên Minh Châu Âu</option>
                        </select>
                    </form>
